# OpenClaw API Environment Variables

# Server
NODE_ENV=development
API_PORT=2222
API_URL=http://localhost:2222
DASHBOARD_URL=http://localhost:5173

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/openclaw

# Hetzner Cloud
HETZNER_API_TOKEN=your_hetzner_api_token_here

# Cloudflare DNS
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here
CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id_here
CLOUDFLARE_DOMAIN=yourdomain.com

# Authentication
JWT_SECRET=your_jwt_secret_here_min_32_chars
WHMCS_API_KEY=your_whmcs_api_key_here
WHMCS_HMAC_SECRET=your_whmcs_hmac_secret_here

# OpenClaw
OPENCLAW_VERSION=latest
