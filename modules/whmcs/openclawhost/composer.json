{
    "name": "openclawhost/whmcs-module",
    "description": "WHMCS Provisioning Module for OpenClaw Host - AI-powered VPS management",
    "type": "whmcs-module",
    "keywords": ["whmcs", "provisioning", "vps", "ai", "hosting"],
    "homepage": "https://openclaw.host",
    "license": "MIT",
    "authors": [
        {
            "name": "OpenClaw Team",
            "email": "support@openclaw.host",
            "homepage": "https://openclaw.host"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "OpenClawHost\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenClawHost\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit tests/",
        "test-coverage": "phpunit --coverage-html coverage tests/"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
